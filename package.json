{
  "name": "all-chess",
  "version": "1.0.0",
  "scripts": {
    "start:dev": "run-p client:watch server:run",
    "start:prod": "run-s client:build server:run",

    "client:watch": "parcel watch parcel-entry.html --cache-dir .parcel-cache --dist-dir ./public --public-url /public",
    "client:build": "parcel build parcel-entry.html --cache-dir .parcel-cache --dist-dir ./public --public-url /public",

    "server:run": "npm run ts:run app/server/index.ts",

    "db:execute-sql": "npm run ts:run scripts/execute-sql.ts",
    "db:migrate": "sequelize db:migrate --debug",
    "db:migrate:create": "sequelize migration:create --debug --name",
    "db:migrate:undo": "sequelize db:migrate:undo --debug",
    "db:migrate:undo:all": "sequelize db:migrate:undo:all --debug",

    "db:seed": "sequelize db:seed:all --debug",
    "db:seed:create": "sequelize seed:create --debug --name",
    "db:seed:rerun": "run-s db:seed:undo:all db:seed",
    "db:seed:undo:all": "sequelize db:seed:undo:all --debug",

    "db:create": "sequelize db:create --debug",
    "db:init": "run-s db:create db:migrate db:seed",
    "db:reset": "run-s db:drop db:init",
    "db:reset:content": "run-s db:migrate:undo:all db:migrate db:seed",
    "db:drop": "sequelize db:drop --debug",

    "calc:pieces-worth": "npm run ts:run scripts/get-pieces-worth.ts",

    "games:remove": "npm run ts:run scripts/remove-games.ts",

    "lint": "eslint . --ext .ts --ext .tsx",
    "lint:fix": "eslint . --ext .ts --ext .tsx --fix",

    "ts:check": "tsc --noEmit",
    "ts:run": "ts-node --transpile-only -r tsconfig-paths/register"
  },
  "alias": {
    "client": "./app/client",
    "server": "./app/server",
    "shared": "./app/shared"
  },
  "dependencies": {
    "@babel/polyfill": "^7.10.4",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "bcryptjs": "^2.4.3",
    "classnames": "^2.2.6",
    "connect-redis": "^3.4.2",
    "express-session": "^1.17.1",
    "font-awesome": "^4.7.0",
    "glicko2": "^0.8.4",
    "history": "^4.10.1",
    "howler": "^2.2.0",
    "koa": "^2.13.0",
    "koa-bodyparser": "^4.3.0",
    "koa-compose": "^4.1.0",
    "koa-mount": "^4.0.0",
    "koa-static": "^5.0.0",
    "lodash": "^4.17.19",
    "moment": "^2.27.0",
    "nodemailer": "^6.4.16",
    "pg": "^8.4.1",
    "pug": "^2.0.4",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "redis": "^2.8.0",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "sequelize": "^6.3.3",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "ts-node": "^8.10.2",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.12.1",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.12.1",
    "@types/autoprefixer": "^6.7.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/classnames": "^2.2.10",
    "@types/connect-redis": "0.0.7",
    "@types/express-session": "^1.17.0",
    "@types/history": "^4.7.6",
    "@types/howler": "^2.2.1",
    "@types/koa": "^2.11.3",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-compose": "^3.2.5",
    "@types/koa-mount": "^4.0.0",
    "@types/koa-static": "^4.0.1",
    "@types/lodash": "^4.14.157",
    "@types/node": "^12.12.54",
